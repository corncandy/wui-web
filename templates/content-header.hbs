<ul class="wui-breadcrumb">
  <li><a href="home.html">{{home}}</a></li>
  {{#each paths}}
    {{#if url}}
      <li><i class="fa fa-angle-right" /></li>
      <li><a href="{{url}}">{{name}}</a></li>
    {{else}}
      <li><i class="fa fa-angle-right" /></li>
      <li>{{name}}</li>
    {{/if}}
  {{/each}}
</ul>
